<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".signup.SignUpFragment"
        android:clickable="true"
        android:focusable="true"
        android:background="@android:color/black">

    <include
            layout="@layout/view_signup_nickname"/>

    <include
            android:id="@+id/view_signup_email"
            android:visibility="gone"
            layout="@layout/view_signup_email"/>

    <include
            android:id="@+id/view_signup_password"
            android:visibility="gone"
            layout="@layout/view_signup_password"/>

    <include
            android:id="@+id/view_signup_school"
            android:visibility="gone"
            layout="@layout/view_signup_school"/>

    <include
            android:id="@+id/view_signup_idol"
            android:visibility="gone"
            layout="@layout/view_signup_idol"/>

</FrameLayout>